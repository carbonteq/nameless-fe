{
	"extends": "@tsconfig/next/tsconfig.json",
	"compilerOptions": {
		"target": "es5",
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
